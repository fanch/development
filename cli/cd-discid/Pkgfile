# Depends on:

description="Backend utility to get cddb information"
url="http://lly.org/~rcw/cd-discid/"
packager="greg<greg@nutyx.org>"

name=cd-discid
version=0.9
release=1

source=(http://lly.org/~rcw/$name/${name}_$version.orig.tar.gz)
release=1

build () {

    cd $name-$version
release=1
    make
 
	}

package () {

cd $name-$version
release=1
make DESTDIR=$PKG \
        PREFIX=/usr \
        mandir=$PKG/usr/man/man1 \
        install
       } 
        
