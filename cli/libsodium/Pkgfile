# Depends on: 

description="P(ortable|ackageable) NaCl-based crypto library"
url="https://github.com/jedisct1/libsodium"
packager="greg<greg@nutyx.org>"
contributors=""

run=()



name=libsodium
version=1.0.15
release=1

)

prepare() {
echo "Fonction vide, normal..."
}

build() {
cd $name-$version
release=1
./configure --prefix=/usr
make
}

package() {
cd $name-$version
release=1
make DESTDIR=$PKG install
}

uptodate() {
echo "Fonction  vide, normal..." 
}

