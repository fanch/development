# Depends on: python-setuptools

description="Parse RSS and Atom feeds in Python"
url="https://github.com/kurtmckee/feedparser"
packager="remy <remy@nutyx.org>"

name=python-mutagen
version=1.39
release=1


build() {

cd mutagen-$version
release=1
python3 setup.py install --root=$PKG
rm -r $PKG/usr/{bin,share}
}
