contributors="Alienus"

name=python-cups
version=1.9.73
release=1

run=(python-requests)

source=(
http://cyberelk.net/tim/data/pycups/pycups-$version.tar.bz2)
release=1

build() {
    cd pycups-$version
release=1
    python3 setup.py install --prefix=/usr --root=$PKG
}
